{
    "inbounds": [
      {
        "tag": "VMess-TCP.json",
        "port": TAG_PORT,
        "listen": "0.0.0.0",
        "protocol": "vmess",
        "settings": {
          "clients": [
            {
              "id": "TAG_ID"
            }
          ]
        },
        "streamSettings": {
          "network": "tcp",
          "tcpSettings": {
            "header": {
              "type": "none"
            }
          }
        },
        "sniffing": {
          "enabled": true,
          "destOverride": [
            "http",
            "tls"
          ]
        }
      }
    ]
  }